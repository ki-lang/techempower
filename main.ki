
use ki:http;

func handler(http:Request req) http:Response{

	return http:Response{
		body: "Hello from example"
	};
}

func main() i32 {

	@ s = http:Server.init("127.0.0.1", 8088, handler) or panic "Failed to start server";
	s.start() or return {
		println("Failed to start http server");
		return 1;
	};

	return 0;
}
